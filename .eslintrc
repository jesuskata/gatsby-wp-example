{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "document": "true"
  },
  "rules": {
    "no-nested-ternary": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "function-paren-newline": "off",
    "import/no-extraneous-dependencies": "off",
    "max-len": [1, 120],
    "no-param-reassign": "off",
    "no-unused-vars": [2, {args: none}],
    "react/destructuring-assignment": "off",
    "react/require-default-props": "off"
  },
  "env": {
    "browser": true
  }
}
